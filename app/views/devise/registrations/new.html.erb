<div class='container' id='sign-up-form'>

  <h2 class="new_text">Sign up</h2>
  <div class="bord new" style="background-color: white">
    <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= f.input :username, autofocus: true %>
      <%= f.input :email, autofocus: true %>
      <%= f.input :password %>
      <%= f.input :password_confirmation, autocomplete: "off"%>
      <%= f.input :age, collection: 10..100 %>
      <%= f.input :city, autofocus: true %>
      <%= f.input :state, collection: us_states, autofocus: true %>
      <%= f.input :zipcode, autofocus: true %>
      <%= f.input :image %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>